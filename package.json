{
  "name": "workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build demo --configuration production",
    "test": "ng test",
    "test:unit": "ng test ngx-joyride --browsers=ChromeHeadless --watch=false",
